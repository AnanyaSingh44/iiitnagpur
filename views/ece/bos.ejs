<%- include('../partials/head2') %> 
<body>
    <%- include('../partials/nav') %> 
    <div class="main2 w-full min-h-screen bg-white text-black">
        <div class="w-full lg:h-[100px] md:h-[80px]"></div>

        <div class="mainContent w-full">
            <div class="routes w-full px-44 flex items-center gap-4 bg-[#00457b] py-5">
                <a class="text-sm font-medium hover:text-blue-400 uppercase inline-block text-zinc-500" href="#">
                    Home <i class="ri-arrow-right-s-line text-md"></i>
                </a>
                <a class="text-sm font-medium hover:text-blue-400 uppercase inline-block text-zinc-500" href="#">
                    Basic Sciences <i class="ri-arrow-right-s-line text-md"></i>
                </a>
                <a class="text-sm font-medium hover:text-blue-400 uppercase inline-block text-zinc-500" href="#">
                    Bos
                </a>
            </div>

            <div class="content px-44 mt-16 mb-10">
                <h2 class="text-2xl uppercase font-bold text-black">Bos</h2>
                <div class="w-full h-[1px] bg-zinc-300 mt-2"></div>

                <div class="overflow-x-auto mt-8">
                    <table class="w-full border border-gray-300">
                        <thead>
                            <tr class="bg-gray-100 border-b border-gray-300">
                                <th class="px-6 py-4 border-r border-gray-300 text-left font-semibold">Sr. No.</th>
                                <th class="px-6 py-4 border-r border-gray-300 text-left font-semibold">Name</th>
                                <th class="px-6 py-4 border-r border-gray-300 text-left font-semibold">Position</th>
                                <th class="px-6 py-4 text-left font-semibold">Designation</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% const members = [
                                { name: "Dr. Harsh Goud", position: "Chairman", designation: "Head of Department, ECE" },
                                { name: "Dr. Tausif Diwan", position: "Special Invitee", designation: "Associate Dean, IIITN" },
                                { name: "Shri Vikas Naiyar", position: "Expert Member", designation: "Industry Expert" },
                                { name: "Dr. Vishal Satpute", position: "Expert Member", designation: "Associate Professor (ECE), VNIT Nagpur" },
                                { name: "Dr. Nishat Ansari", position: "Special Invitee", designation: "Head of Department, CSE" },
                                { name: "Dr. Prasad Joshi", position: "Special Invitee", designation: "Head of Department , BS" },
                                { name: "Dr. Mayur Parate", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Tapan Jain", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Rashmi Pandhare", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Paritosh Peshwe", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Parul Sahare", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Nikhil Agrawal", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Girish Ghivela", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Sushmita Dandeliya", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Shankar Bhattacharjee", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Rajanish Kumar Singh", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Khuraijam Nelson Singh", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Nikhil Dhengre", position: "Member", designation: "Assistant Professor" },
                                { name: "Dr. Bhukya Venkanna Naik", position: "Member", designation: "Assistant Professor" }
                            ]; %>

                            <% members.forEach((member, index) => { %>
                                <tr class="border-b border-gray-300">
                                    <td class="px-6 py-4 border-r border-gray-300"><%= index + 1 %></td>
                                    <td class="px-6 py-4 border-r border-gray-300"><%= member.name %></td>
                                    <td class="px-6 py-4 border-r border-gray-300"><%= member.position %></td>
                                    <td class="px-6 py-4"><%= member.designation %></td>
                                </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <%- include('../partials/footer') %> 
</body>
<%- include('../partials/tail2') %> 
