<%- include('./partials/head') %>

  <body>
    <div id="main">
      <div id="languageSwitcher"
        class="cursor-pointer fixed bottom-[20px] left-[20px] z-[99] px-[10px] py-[8px] space-y-4 lang bg-white">
        <div class="md:text-[1vw] text-[3.5vw] flex items-center gap-[5px]">
          English
        </div>
        <div class="md:text-[1vw] text-[3.5vw] flex items-center gap-[5px]">
          <a href="https://iiitn-ac-in.translate.goog/?_x_tr_sl=auto&_x_tr_tl=hi&_x_tr_hl=en">Hindi</a>
        </div>
        <div class="md:text-[1vw] text-[3.5vw] flex items-center gap-[5px]">
          <a href="https://iiitn-ac-in.translate.goog/?_x_tr_sl=auto&_x_tr_tl=bn&_x_tr_hl=en">Bengali</a>
        </div>
        <div id="mainLang" class="md:text-[1vw] text-[3.5vw] flex items-center gap-[5px]" onclick="toggleLanguages()">
          Languages <i id="arrow" class="ri-arrow-up-s-line font-[18px] transition-transform"></i>
        </div>
      </div>

      <div id="page1" class="w-full h-screen">
        <%- include('./partials/nav') %>

          <div class="hero w-full h-full">
            <div
              class="dets-box bg-[black]/60 md:w-[300px] md:h-[300px] w-[200px] h-[200px] absolute bottom-[40px] right-[50px] z-[99] text-white p-[10px]">
              <p class="md:py-4 py-2 lg:text-[16px] text-[10px]">
                We're delighted to have you here! Explore our vibrant campus, academic programs, research initiatives,
                and student life. Whether you're a prospective student, alumni, or just curious, we invite you to
                discover what makes IIIT Nagpur a hub of learning, innovation, and excellence.
              </p>
            </div>
            <div
              class="dets flex items-center justify-center w-[60px] h-[60px] rounded-full bg-[black]/90 absolute bottom-[30px] right-[40px] z-[100] text-white">
              <i class="ri-lightbulb-line bulb text-white text-[25px]"></i>
            </div>
            <img class="w-full h-full object-cover" src="../images/index/8.webp" alt="" />
          </div>
      </div>


      <div id="announcements" class="w-full pt-10 md:mb-24 mb-16">
        <h1 class="md:text-[2vw] text-[6vw] md:pl-[6vw] pl-[12vw] text-[#00457b] font-medium">Announcements</h1>
        <div class="flex md:flex-row flex-col justify-evenly w-full mx-auto md:px-12 px-2 md:my-2">
          <!-- first 2 from studentNotice -->
          <div class="ann1 md:w-[32%] px-2 w-full relative">
            <% studentNotice.slice(0,2).forEach(function(item){ %>
              <div class="flex items-center py-2 md:gap-4 gap-2">
                <i class="ri-arrow-right-s-fill md:text-[2vw] text-[6vw] text-[#f26e32]"></i>
                <a href="<%= item.link %>" class="md:text-[1.2vw] text-[4.5vw]" target="_blank">
                  <%= item.title %>
                </a>
              </div>
              <% }) %>
          </div>
          <!-- next 2 from studentNotice -->
          <div class="ann1 md:w-[32%] px-2 w-full relative">
            <% studentNotice.slice(2,4).forEach(function(item){ %>
              <div class="flex items-center py-2 md:gap-4 gap-2">
                <i class="ri-arrow-right-s-fill md:text-[2vw] text-[6vw] text-[#f26e32]"></i>
                <a href="<%= item.link %>" class="md:text-[1.2vw] text-[4.5vw]" target="_blank">
                  <%= item.title %>
                </a>
              </div>
              <% }) %>
          </div>
          <!-- last 2 from facultyNotice -->
          <div class=" md:w-[32%] px-2 w-full relative">
            <% facultyNotice.slice(0,2).forEach(function(item){ %>
              <div class="flex items-center py-2 md:gap-4 gap-2">
                <i class="ri-arrow-right-s-fill md:text-[2vw] text-[6vw] text-[#f26e32]"></i>
                <a href="<%= item.link %>" class="md:text-[1.2vw] text-[4.5vw]" target="_blank">
                  <%= item.title %>
                </a>
              </div>
              <% }) %>
          </div>
        </div>

        <div class="flex justify-center my-2">
          <a href="/readMore">
            <button
            class="px-4 py-2 text-[#00457b] bg-white font-semibold rounded-lg border-2 border-[#00457b] hover:bg-[#003366] hover:scale-105 hover:text-white transition flex items-center gap-2 lg:text-[1vw] md:text-[1.8vw] text-[4vw]">
            Read More<i class="ri-arrow-right-line"></i>
          </button>
          </a>
        </div>
      </div>

      <div id="page3" class="page2b md:px-8 px-4  pb-4">
        <h1 class="main font-bold text-[#00457b] text-center lg:text-[2.5vw] md:text-[3vw] text-[5vw] mb-2">
